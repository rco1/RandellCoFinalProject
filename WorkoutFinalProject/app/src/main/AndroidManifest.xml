<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rco1.com.workoutfinalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Workout.WorkoutTypes.WorkoutStandardActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            />
        <activity android:name=".Intro.IntroSetup" />
        <activity
            android:name=".Workout.WorkoutTypes.WorkoutRunningActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".Workout.WorkoutTypes.WorkoutHoldActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".Regiment.RegimentAddActivity" />
        <activity android:name=".Regiment.RegimentViewActivity" />

        <activity android:name=".Workout.WorkoutMain" />
        <activity android:name=".Workout.WorkoutMapper"></activity>
    </application>

</manifest>